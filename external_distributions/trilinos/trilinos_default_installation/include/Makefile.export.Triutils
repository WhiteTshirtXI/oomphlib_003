Triutils_MPI_LIBRARY_DIRS=
Triutils_MPI_INCLUDE_DIRS=
Triutils_MPI_EXEC=
Triutils_MPI_EXEC_MAX_NUMPROCS=
Triutils_MPI_EXEC_NUMPROCS_FLAG=

## ---------------------------------------------------------------------------
## Set useful general variables 
## ---------------------------------------------------------------------------

## The packages enabled for this project
Triutils_PACKAGE_LIST= Triutils Epetra

## The TPLs enabled for this project
Triutils_TPL_LIST= LAPACK BLAS

